   "source": [
    "def std_deviation(my_list):\n",
    "    mean = sum(my_list)/len(my_list)\n",
    "    variance = 0\n",
    "    for x in my_list:\n",
    "        variance+=(x-mean)**2\n",
    "    return (variance/len(my_list))**0.5"
   ]
